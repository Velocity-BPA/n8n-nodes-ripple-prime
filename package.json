{
  "name": "n8n-nodes-ripple-prime",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Ripple Prime (formerly Hidden Road) institutional prime brokerage platform providing 18 resources and 200+ operations for multi-asset trading, cross-margining, treasury management, and regulatory compliance. Includes FIX protocol, WebSocket streaming, and bitemporal data support.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-ripple-prime.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-ripple-prime#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-ripple-prime/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "ripple",
    "ripple-prime",
    "hidden-road",
    "prime-brokerage",
    "institutional-trading",
    "multi-asset",
    "cross-margining",
    "fx-trading",
    "crypto-trading",
    "derivatives",
    "fixed-income",
    "treasury-management",
    "fix-protocol",
    "websocket",
    "bitemporal",
    "buidl",
    "rlusd",
    "route28",
    "compliance",
    "fintech"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RipplePrimeApi.credentials.js",
      "dist/credentials/RipplePrimeFix.credentials.js",
      "dist/credentials/RipplePrimeWebSocket.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RipplePrime/RipplePrime.node.js",
      "dist/nodes/RipplePrime/RipplePrimeTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 2 \"nodes/**/*.svg\" dist/nodes",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.17.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.17.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
